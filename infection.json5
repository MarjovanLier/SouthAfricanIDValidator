{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "minMsi": 91,
    "minCoveredMsi": 91,
    "mutators": {
        "@default": true,
        "@boolean": true,
        "@conditional_boundary": true,
        "@conditional_negotiation": true,
        "@function_signature": true,
        "@number": true,
        "@operator": true,
        "@regex": true,
        "@removal": true,
        "@return_value": true,
        "@sort": true,
        "@unwrap": true,
        "@loop": true,
        "@identical": true,
        "@equal": true
    },
    "testFramework":"phpunit",
    "logs": {
        "html": "reports/infection.html"
    }
}